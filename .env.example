# Service 7 / Service 15 Environment Variables

# OpenAI Configuration
OPENAI_API_KEY=your_openai_api_key_here

# Google Maps/Solar API
GOOGLE_MAPS_API_KEY=your_google_maps_api_key_here

# Tavily Search API (for web research)
TAVILY_API_KEY=your_tavily_api_key_here

# Serper API (optional - for additional search)
SERPER_API_KEY=your_serper_api_key_here

# Langfuse Tracing Configuration
# Get your keys from https://cloud.langfuse.com
LANGFUSE_PUBLIC_KEY=your_langfuse_public_key_here
LANGFUSE_SECRET_KEY=your_langfuse_secret_key_here
LANGFUSE_HOST=https://cloud.langfuse.com

# Server Configuration
PORT=8015
HOST=0.0.0.0

# Snowflake Database Configuration (optional)
SNOWFLAKE_ACCOUNT=your_snowflake_account
SNOWFLAKE_USER=your_snowflake_user
SNOWFLAKE_PASSWORD=your_snowflake_password
SNOWFLAKE_DATABASE=your_database
SNOWFLAKE_SCHEMA=your_schema
SNOWFLAKE_WAREHOUSE=your_warehouse
